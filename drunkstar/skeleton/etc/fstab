# /etc/fstab: static file system information.
#
# <file system> <mount pt>     <type>	<options>         <dump> <pass>
/dev/root       /              ext2	defaults,ro,noatime,sync       0      1
/dev/sda2       /root/logs     ext2     defaults,rw,noatime,sync       0      1
proc		/proc	       proc     defaults,noatime	  0	 0
devpts		/dev/pts       devpts   defaults,noatime,gid=5,mode=620	  0	 0
tmpfs           /tmp           tmpfs    defaults,noatime          0      0
sysfs		/sys	       sysfs    defaults,noatime	  0	 0
